{
	"name": "sample-action-client",
	"displayName": "Sample Action Client",
	"version": "0.0.3",
	"description": "Sample Action Client",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/app-studio-toolkit",
		"directory": "sample-action-client"
	},
	"publisher": "SAPOSS",
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package ."
	},
	"contributes": {
		"commands": [
			{
				"title": "Perform Action Immediately",
				"command": "perform.action.now"
			},
			{
				"title": "Schedule Action",
				"command": "schedule.action"
			},
			{
				"title": "Get Configuration Parameter",
				"command": "get.parameter"
			}
		]
	},
	"activationEvents": [
		"onCommand:perform.action.now",
		"onCommand:schedule.action"
	],
	"devDependencies": {
		"@sap-devx/app-studio-toolkit-types": "0.0.5",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.9.7",
		"vsce": "^1.69.0",
		"vscode-test": "^1.2.2"
	},
	"extensionDependencies": [
		"SAPOSS.app-studio-toolkit"
	],
	"engines": {
		"vscode": "^1.44.2"
	},
	"BASContributes": {
		"actions": [
			{
				"id": "openSettingsAction",
				"actionType": "COMMAND",
				"command": "workbench.action.openGlobalSettings"
			}
		]
	}
}
