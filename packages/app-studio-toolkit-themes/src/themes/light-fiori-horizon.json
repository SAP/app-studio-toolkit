  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#556B82"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#F56A00"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#1A2733"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#6C32A9"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#256F3A"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#2395FF"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#EE3939"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#1A2733"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F56A00"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#1A2733"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AA0808"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AA0808"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AA0808"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AA0808"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#EE3939",
        "foreground": "#ffffff",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#AA0808"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#1A2733"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#256F3A"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#F56A00"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#256F3A"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1A2733"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1A2733"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#FFEAF4",
        "foreground": "#AA0808"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#F5FAE5",
        "foreground": "#256F3A"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#FFF8D6",
        "foreground": "#F56A00"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#F5F6F7",
        "background": "#0057D2"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#6C32A9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0057D2"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#0057D2"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#AA0808"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#032f62",
        "fontStyle": "underline"
      }
    }
  ]
}
