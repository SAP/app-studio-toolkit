{
    "name": "MyBookshop",
    "version": "1.0.0",
    "description": "My bookshop app",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap-cloud-sdk/http-client": "^3.15.0",
        "@sap-cloud-sdk/util": "^3.15.0",
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/low-code-event-handler": "^1.2.4",
        "@sap/xsenv": ">=3.3.2 <3.3.3",
        "@sap/xssec": "^3.6.1",
        "express": "^4",
        "hdb": "^0.19.8",
        "passport": "^0.7.0",
        "@sap/mdk-tools": "^1.3.1"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/dev-cap-tools": "^1.31.0"
    },
    "scripts": {
        "start": "cds-serve",
        "start-local": "npm run generate-entry-point && npm run cds-watch",
        "generate-entry-point": "dev-cap-tools gen-entrypoint",
        "cds-watch": "cds watch --open"
    },
    "cds": {
        "requires": {
            "[production]": {
                "db": "hana"
            },
            "app-service": {
                "impl": "@sap/low-code-event-handler"
            },
            "auth": {
                "[development]": {
                    "kind": "dummy"
                },
                "[production]": {
                    "kind": "xsuaa"
                }
            }
        },
        "features": {
            "fetch_csrf": true
        }
    },
    "lcap": true
}